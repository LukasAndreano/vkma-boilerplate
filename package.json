{
  "name": "vkma-boilerplate",
  "private": true,
  "version": "1.0.0",
  "homepage": "./",
  "scripts": {
    "dev": "vite --port=3000",
    "build": "yarn prettier && vite build --base=./",
    "preview": "vite preview",
    "odr": "env ODR=1 yarn build && tar -cvzf odr.zip -C ./odr-dist/ .",
    "prettier": "npx prettier --write .",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=http --ws-protocol=wss --host=localhost --port=3000",
    "deploy": "yarn build && vk-miniapps-deploy",
    "kokateam-deploy": "yarn build && kokateam-deploy"
  },
  "dependencies": {
    "@reyzitwo/react-router-vkminiapps": "^1.4.7",
    "@vkontakte/icons": "^2.1.1",
    "@vkontakte/vk-bridge": "^2.7.2",
    "@vkontakte/vkui": "^4.40.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.7.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^3.0.0",
    "@vkontakte/vk-miniapps-deploy": "^0.0.26",
    "@vkontakte/vk-tunnel": "^0.1.3",
    "eruda": "^2.7.4",
    "eruda-code": "^2.1.0",
    "eruda-dom": "^2.0.0",
    "kokateam-deploy": "^1.0.21",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "vite": "^4.0.0",
    "vite-plugin-singlefile": "^0.13.1",
    "vite-svg-loader": "^3.6.0"
  }
}
